<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomViewPager">
        <attr name="swipe_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="SideSpinner">
        <attr name="values" format="reference" />
    </declare-styleable>
    <!--Custom button-->
    <declare-styleable name="MainPageCustomButton">
        <attr name="pageName" format="string" />
        <attr name="pageIcon" format="reference" />
    </declare-styleable>

</resources>